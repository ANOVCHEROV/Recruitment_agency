@model RecAgency.Models.Vacancy

@{
    ViewBag.Title = "Delete";
}


<h3>Вы уверены, что хотите удалить эту запись?</h3>
<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Заголовок
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Name)
        </dd>

        <dt>
            Область деятельности 
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Field)
        </dd>

        <dt>
            Занятость
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Schedule)
        </dd>

        <dt>
            Опыт
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Experience)
        </dd>

        <dt>
            Адрес 
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Address)
        </dd>

        <dt>
           Зарплата
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Salary)
        </dd>

        <dt>
            Дата последнего редактирования
        </dt>

        <dd>
            @Html.DisplayFor(model => model.DateOfPublication)
        </dd>

        <dt>
            Информация
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Info)
        </dd>

        <dt>
            Название организации
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Organization)
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Удалить" class="btn btn-default" /> |
            <div>
                @Html.ActionLink("Назад к списку вакасий", "List", "Vac")
            </div>
            <div>
                @Html.ActionLink("К стартовой странице", "Index", "Home")
            </div>
        </div>
    }
</div>
